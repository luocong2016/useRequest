{
  "name": "@yx-frame/userequest",
  "version": "0.0.1",
  "description": "vue3 ahook useRequest",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.mjs",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "dev": "node ./build.js -w",
    "build:types": "tsc -p ./tsconfig.json --emitDeclarationOnly",
    "build:bundle": "node ./build.js",
    "build": "yarn clean && yarn build:bundle && yarn build:types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luocong2016/useRequest.git"
  },
  "keywords": [
    "vue3",
    "hook",
    "request"
  ],
  "author": "lutz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luocong2016/useRequest/issues"
  },
  "homepage": "https://github.com/luocong2016/useRequest#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.14.1",
    "esbuild": "^0.16.10",
    "typescript": "^4.8.2",
    "loadsh": "^0.0.4",
    "vue": "^3.2.47"
  }
}
